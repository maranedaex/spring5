public class Test {

	public static void main(String[] args) {
		int x = 5;
		int y = 5;
				
		y /= ++x;
		System.out.println(x);
	} //fin
	
	 public class MiPrimerApplet extends Applet {  
		   public void paint(Graphics g){
		     g.drawString("Hola Mundo",40,80);
		   }
		}

}
